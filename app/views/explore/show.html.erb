<% content_for :custom_assets do %>
  <%= javascript_include_tag "explorer" %>
<% end %>
<section class="mapping_info">
  <div class="comments">
    <a class="question" href="#">I have a question on this mapping</a>
    <div class="form_container">
      <span>Submit a comment regarding this mapping:</span>
      <%= form_tag review_path(@mapping.id) do %>
        <%= text_area_tag :comment %>
        <%= submit_tag 'submit' %>
      <% end %>
    </div>
    <div class="comment_messages">

    </div>
  </div>
</section>
<div class="mapping_links_container">
  <div class="left">

  </div>
  <div class="right">

  </div>
</div>
<iframe id="old" class="left" src=""></iframe>
<iframe id="new" class="right" src=""></iframe>
